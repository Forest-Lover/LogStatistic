{
    "input" : {
        "root_path" : "../../server/Bin/cluster_scripts/linux/service_cluster_0/",
        "file_filter" : {
            "etcd" : [
                "log/etcd.log*"
            ]
        },
        "field_mapping" : "ETCD"
    },
    "output" : {
        "toplist" : [
            {
                "field" : "TIMESTAMP",
                "count" : 10,
                "merge" : "TIME-3600"
            },
            {
                "field" : "LEVEL",
                "count" : 5,
                "merge" : "EQUAL"
            },
            {
                "field" : "MODULE",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "CALLER",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "TIME_SPENT",
                "count" : 10,
                "merge" : "ROUND_DECIMAL-0"
            },
            {
                "field" : "REMOTE",
                "count" : 50,
                "merge" : "EQUAL"
            },
            {
                "field" : "RESPONSE_TYPE",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "REQUEST_COUNT",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "REQUEST_SIZE",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "RESPONSE_COUNT",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "RESPONSE_SIZE",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "REQUEST_CONTENT",
                "count" : 10,
                "merge" : "EQUAL"
            },
            {
                "field" : "ERROR",
                "count" : 10,
                "merge" : "K_MEANS-30"
            },
            {
                "field" : "MESSAGE",
                "count" : 10,
                "merge" : "K_MEANS-30"
            }
        ],
        "merge_file" : false,
        "write_log_file" : true
    }
}

