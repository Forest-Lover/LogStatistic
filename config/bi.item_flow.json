{
    "input" : {
        "root_path" : "../../server/Bin/cluster_scripts/windows/cluster_current/",
        "file_filter" : {
            "game" : [
                "log/game*.txt"
            ]
        },
        "line_pattern" : "^\\[.*?\\]$",
        "line_filter" : "\\[BI\\]",
        "line_extract" : "(?<=\\[)\\{.*\\}(?=\\])",
        "field_mapping" : "BI_ITEM_FLOW",
        "field_filters" : [
            {
                "field" : "event",
                "match" : "item_flow"
            }
        ]
    },
    "output" : {
        "toplist" : [
            {
                "field" : "event",
                "count" : 1000000,
                "merge" : "NONE"
            },
            {
                "field" : "detail.item_id",
                "count" : 1000000,
                "merge" : "NONE"
            }
        ],
        "merge_file" : false,
        "write_log_file" : true
    }
}

