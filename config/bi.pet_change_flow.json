{
    "input" : {
        "root_path" : "../../server/Bin/cluster_scripts/*/cluster_current/",
        "file_filter" : {
            "game" : [
                "log/game*"
            ]
        },
        "line_pattern" : "^\\[.*?\\]$",
        "line_filter" : "\\[BI\\]",
        "line_extract" : "(?<=\\[)\\{.*\\}(?=\\])",
        "field_mapping" : "BI_PET_CHNAGE_FLOW",
        "field_filters" : [
            {
                "field" : "event",
                "match" : "pet_change_flow"
            }
        ]
    },
    "output" : {
        "toplist" : [
            {
                "field" : "event",
                "count" : 1000000,
                "merge" : "EQUAL"
            },
            {
                "field" : "detail.pet_id",
                "count" : 1000000,
                "merge" : "EQUAL"
            },
            {
                "field" : "detail.prop_info.0",
                "count" : 1000000,
                "merge" : "EQUAL"
            }
        ],
        "merge_file" : false,
        "write_log_file" : true
    }
}

